<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medard.mycanvasapp2">
    -
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        -
        <activity android:name="com.example.gijs.super42.MainActivity">
            -
        </activity>
        <activity android:name="com.example.gijs.super42.StartUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.gijs.super42.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MenuActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.gijs.super42.DieActivity"></activity>
    </application>

</manifest>